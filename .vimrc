set clipboard=unnamed
set mouse=a
set noswapfile
set background=dark
set number relativenumber
set ruler
set undofile
set undodir=~/.vim/undodir
set autoindent
set nocompatible
set redrawtime=10000

filetype plugin indent on

set tabstop=2
set softtabstop=2
set expandtab
set shiftwidth=2

syntax on

let g:nvcode_termcolors=256
colorscheme peachpuff
